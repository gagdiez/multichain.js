{
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "theme": "default",
  "name": "ChainSig.js Documentation",
  "readme": "README.md",
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "__tests__/**/*",
    "__mocks__/**/*",
    "examples/**/*",
    "scripts/**/*"
  ],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": false,
  "includeVersion": true,
  "sort": ["source-order"],
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Chain Adapters",
    "Contracts", 
    "Utils",
    "Types",
    "Other"
  ],
  "gitRevision": "main",
  "gitRemote": "origin",
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  }
}